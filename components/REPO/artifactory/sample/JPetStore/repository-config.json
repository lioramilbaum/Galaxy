{
  "key": "lmb-repo",
  "rclass" : "local",
  "description": "The local repository public description",
  "notes": "Some internal notes",
  "includesPattern": "**/*",
  "excludesPattern": "",
  "repoLayoutRef" : "maven-2-default",
  "enableNuGetSupport" : false,
  "enableGemsSupport" : false,
  "enableNpmSupport" : false,
  "enableDebianSupport" : false,
  "debianTrivialLayout" : false,
  "enablePypiSupport" : false,
  "enableDockerSupport" : false,
  "checksumPolicyType": "client-checksums",
  "handleReleases": true,
  "handleSnapshots": true,
  "maxUniqueSnapshots": 0,
  "snapshotVersionBehavior": "non-unique",
  "suppressPomConsistencyChecks": false,
  "blackedOut": false,
  "propertySets": [],
  "archiveBrowsingEnabled" : false,
  "calculateYumMetadata" : false,
  "yumRootDepth" : 0
}